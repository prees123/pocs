  "eslintConfig": {
    "extends": [
      "standard",
      "standard-jsx",
      "plugin:jsx-a11y/recommended",
      "plugin:flowtype/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-console": 2,
      "flowtype/delimiter-dangle": [
        2,
        "always-multiline"
      ]
    },
    "globals": {
      "afterEach": true,
      "beforeEach": true,
      "describe": true,
      "expect": true,
      "it": true,
      "jest": true
    },
    "plugins": [
      "jsx-a11y",
      "flowtype"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/raf.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web"
    }
  },
  "keywords": [
    "react",
    "praxis",
    "react-component",
    "material design",
    "material-ui",
    "target"
  ],
  "license": "UNLICENSED",
  "name": "trellis",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "flow": "node scripts/flow.js",
    "generate": "plop --plopfile generators/index.js",
    "lint": "esw src/ scripts/ config/ generators/",
    "release": "standard-version",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage"
  }
